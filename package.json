{
	"name": "obsidian-sample-template-onboarding",
	"version": "1.0.1",
	"type": "module",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"dev": "tsc -noEmit -skipLibCheck && vite build --watch",
		"build": "tsc -noEmit -skipLibCheck && vite build",
		"postversion": "git push origin HEAD && git push origin --tags",
		"format": "prettier --write .",
		"format:check": "prettier --check .",
		"lint": "eslint . --ext .ts,.tsx,.js,.svelte",
		"lint:fix": "eslint . --ext .ts,.tsx,.js,.svelte --fix",
		"lint-staged": "lint-staged",
		"prepare": "husky"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"@types/node": "^20.14.0",
		"@typescript-eslint/eslint-plugin": "^8.0.0",
		"@typescript-eslint/parser": "^8.0.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "^0.25.10",
		"eslint": "^9.0.0",
		"eslint-plugin-svelte": "^3.12.4",
		"globals": "^16.4.0",
		"husky": "^9.1.7",
		"lint-staged": "^15.2.10",
		"obsidian": "latest",
		"prettier": "^3.3.3",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.39.4",
		"svelte-check": "^4.3.2",
		"svelte-preprocess": "^6.0.3",
		"tslib": "^2.8.0",
		"typescript": "^5.6.0",
		"vite": "^7.1.7",
		"vite-plugin-string": "^1.2.3"
	},
	"lint-staged": {
		"*.{ts,tsx,js,svelte}": [
			"eslint --fix",
			"prettier --write"
		],
		"*.{json,css,md}": [
			"prettier --write"
		]
	}
}
